{
    "name": "paulodiff/rainbow-table-index",
    "description": "Laravel database data encryption with full text search",
    "keywords": [
        "laravel",
        "database",
        "encryption",
        "full",
        "text",
        "search",
        "rainbow-table-index"
    ],
    "homepage": "https://github.com/paulodiff/rainbow-table-index",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Paulodiff",
            "email": "paulo.difficiliora@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Paulodiff\\RainbowTableIndex\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Paulodiff\\RainbowTableIndex\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Paulodiff\\RainbowTableIndex\\RainbowTableIndexServiceProvider"
            ]
        }
    }
}
